{
	"name": "starlight-dot-md",
	"version": "0.1.2",
	"description": "A Starlight plugin that exposes raw markdown files at `.md` URLs.",
	"keywords": [
		"astro",
		"withastro",
		"astro-component",
		"astro-integration",
		"starlight",
		"ai"
	],
	"author": "Shinya Fujino <shf0811@gmail.com> (https://github.com/morinokami)",
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/morinokami/starlight-dot-md.git",
		"directory": "packages/starlight-dot-md"
	},
	"bugs": {
		"url": "https://github.com/morinokami/starlight-dot-md/issues"
	},
	"exports": {
		".": "./dist/index.mjs",
		"./slug.md": "./dist/slug.md.mjs",
		"./slug.mdx": "./dist/slug.mdx.mjs",
		"./slug.mdoc": "./dist/slug.mdoc.mjs"
	},
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"lint": "biome ci",
		"publint": "publint"
	},
	"dependencies": {
		"picomatch": "4.0.3",
		"yaml": "2.8.2"
	},
	"devDependencies": {
		"@astrojs/starlight": "0.37.3",
		"@types/node": "25.0.9",
		"@types/picomatch": "4.0.2",
		"astro": "5.16.11",
		"publint": "0.3.16",
		"tsdown": "0.18.3",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"astro": ">=5.0.0"
	}
}
